<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.fastandroid.myapplication"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Default">
        <activity
            android:name=".MainActivity_"
            android:theme="@style/AppTheme.Default.Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity
            android:name=".SecondActivity_"
            android:theme="@style/AppTheme.Default">
        </activity>
        <activity
            android:name="org.yndongyong.fastandroid.component.image_display.FaSingleImageActivity"
            android:theme="@style/AppTheme.Default"
            android:label="预览单个图片"
        />

        <activity
            android:name="org.yndongyong.fastandroid.component.image_display.FaPicScanActivity"
            android:theme="@style/AppTheme.Default"
            android:label="预览多个图片"
        />
        <activity android:name=".ThirdActivity"
                  android:theme="@style/AppTheme.Default.NoStatusBar">
            
        </activity>
    </application>

</manifest>